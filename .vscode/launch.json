{
  "configurations": [
    {
      "name": "(gdb) Launch transcode.js",
      "type": "cppdbg",
      "request": "launch",
      "program": "/usr/bin/node",
      "args": [
        "transcode.js",
        "data/launch.mp4",
        "out.mp4"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/test",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          },
          {
              "description": "Set Disassembly Flavor to Intel",
              "text": "-gdb-set disassembly-flavor intel",
              "ignoreFailures": true
          }
      ]
    },
    {
      "name": "(gdb) Launch mocha",
      "type": "cppdbg",
      "request": "launch",
      "program": "/usr/bin/node",
      "args": [
        "--expose-gc",
        "${workspaceFolder}/node_modules/mocha/lib/cli/cli.js",
        "-R",
        "tap",
        "--timeout",
        "0",
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
